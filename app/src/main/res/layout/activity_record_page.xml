<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    android:paddingBottom="0dp"
    android:background="@drawable/case1"
    tools:context=".activities.RecordPageActivity">

    <Button
        android:id="@+id/button14"
        android:layout_width="142dp"
        android:layout_height="39dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/button9"
        android:layout_marginEnd="69dp"
        android:background="@drawable/frame2"
        android:text="animate"
        android:textSize="18sp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="142dp"
        android:layout_height="39dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="83dp"
        android:layout_marginStart="38dp"
        android:background="@drawable/frame2"
        android:text="my profile"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="209dp"
        android:layout_height="95dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="List Record"
        android:textColor="@android:color/background_dark"
        android:textSize="30sp"
        android:textStyle="bold" />

    <ListView
        android:id="@+id/listview1"
        android:layout_width="333dp"
        android:layout_height="265dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView3"
        android:layout_marginStart="29dp"
        android:layout_marginTop="6dp" />


    <Button
        android:id="@+id/button5"
        android:layout_width="136dp"
        android:layout_height="38dp"
        android:layout_above="@+id/button14"
        android:layout_toStartOf="@+id/button14"
        android:background="@drawable/frame2"
        android:text="@string/add_new"
        android:textSize="18sp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="142dp"
        android:layout_height="39dp"
        android:layout_above="@+id/button14"
        android:layout_alignStart="@+id/button14"
        android:background="@drawable/frame2"
        android:text="Body picture"
        android:textSize="18sp" />
</RelativeLayout>